{
  "name": "Steam Market SCRIPT",
  "version": "1.0",
  "description": "Build an Extension!",
  "background": {
    "scripts": ["index.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": ["contentscript.js"],
      "matches": ["https://steamcommunity.com/*"]
    }
  ],
  "web_accessible_resources": ["script.js"],
  "manifest_version": 2
}
